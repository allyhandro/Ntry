<div id="view-client-title" class="row">
    <div class = "col-lg-12">
        <h1>Your Clients: </h1>
    </div>
</div>
{{#each client}}
    <div id="client" class="row">
        <div class="col-md-6">
            <p>{{this.name}}</p>
        </div>
        <div class="col-md-6">
            <p>{{this.rent}}</p>
        </div>
    </div>
{{/each}}